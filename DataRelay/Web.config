        </behavior>
      </endpointBehaviors>
    </behaviors>
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
    <services>
      <service name="DataRelay.DataRelay" behaviorConfiguration="basicHttpServiceBehavior">
